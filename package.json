    "@vitest/coverage-c8": "^0.24.3",
    "chalk": "^5.0.1",
    "coveralls": "^3.1.1",
    "cypress": "^10.10.0",
    "cypress-hmr-restarter": "^2.0.4",
    "happy-dom": "^7.6.0",
    "inquirer": "^9.1.2",
    "istanbul-merge": "^1.1.1",
    "quote-it": "^1.0.1",
    "shelljs": "^0.8.5",
    "vite": "^3.1.8",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-istanbul": "^3.0.1",
    "vitest": "^0.24.3"
  },
  "scripts": {
    "start": "VITE_COVERAGE='false' vite",
    "start:coverage": "VITE_COVERAGE='true' start",
    "build": "vite build",
    "test": "NODE_ENV=test vitest",
    "test:coverage": "vitest run --coverage",
    "merge-coverage": "npx istanbul-merge --out coverage/merged/coverage-final.json coverage/unit/coverage-final.json coverage/integration/coverage-final.json",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "cypress": "NODE_OPTIONS=--openssl-legacy-provider cypress open",
    "cypressrun": "NODE_OPTIONS=--openssl-legacy-provider cypress run",
    "release": "standard-version",
    "release-version": "standard-version --dry-run | ts-node scripts/next-release-version.ts",
    "update-copyright-year": "ts-node scripts/update-copyright-year.ts",
    "generate-page": "node scripts/generate-page"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "engines": {
    "npm": "7.x",
    "node": "14.x"
  },
  "nyc": {
    "report-dir": "coverage/integration",
    "all": true,
    "include": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "exclude": [
      "**/*.spec.{js,jsx,ts,tsx}",
      "**/*.stories.{js,jsx,ts,tsx}",
      "src/reportWebVitals.ts"
    ]
  }
}
